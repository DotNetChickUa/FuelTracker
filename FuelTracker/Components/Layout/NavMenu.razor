<MudNavMenu>
    <AuthorizeView>
        <Authorized>
            <MudNavLink Href="/" Match="NavLinkMatch.All">Dashboard</MudNavLink>
            <MudNavLink Href="/profile" Match="NavLinkMatch.Prefix">Profile</MudNavLink>
            <MudNavLink Href="/vehicles" Match="NavLinkMatch.Prefix">Vehicles</MudNavLink>
            <MudNavLink Href="/fuel" Match="NavLinkMatch.Prefix">Fuel</MudNavLink>
            <MudNavLink Href="/logout" Match="NavLinkMatch.Prefix">
                <MudIcon Icon="@Icons.Material.Filled.Logout" Class="me-2"/>
                Logout
            </MudNavLink>
        </Authorized>
        <NotAuthorized>
            <MudNavLink Href="/login" Match="NavLinkMatch.Prefix">
                <MudIcon Icon="@Icons.Material.Filled.Login" Class="me-2" />
                Login
            </MudNavLink>
            <MudNavLink Href="/register" Match="NavLinkMatch.Prefix">
                <MudIcon Icon="@Icons.Material.Filled.PersonAdd" Class="me-2" />
                Register
            </MudNavLink>
        </NotAuthorized>
    </AuthorizeView>
</MudNavMenu>